{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run vercel-simple",
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*\\.(js|json|css|ico|png|jpg|jpeg|svg|webp|gif))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
